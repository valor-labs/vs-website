{
  "project": {
    "version": "1.0.0",
    "name": "vs-website"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "CNAME"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "assets/css/styles.css",
        "assets/css/feedback.css",
        "assets/css/hamburger-menu.css",
        "assets/css/footer.css",
        "assets/css/landing.css",
        "assets/css/forms.css",
        "assets/css/vacancies.css",
        "assets/css/team.css",
        "assets/css/header.css",
        "assets/css/top-menu.css",
        "assets/css/contact.css",
        "assets/css/about.css"
      ],
      "scripts": [
        "assets/js/particles.js",
        "assets/js/helper.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.json"
    }
  ],
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
